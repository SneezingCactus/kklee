{
  "manifest_version": 2,
  "name": "kklee",
  "description": "A Bonk.io mod that extends the functionality of the map editor. Patched for new bonk.io version by SneezingCactus.",
  "author": "kklkkj",
  "homepage_url": "https://github.com/kklkkj/kklee/",
  "version": "1.3",
  "permissions": ["webRequest", "webRequestBlocking", "*://bonk.io/*"],
  "background": {
    "scripts": ["./background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://bonk.io/gameframe-release.html"],
      "js": ["loadInjector.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["injector.js", "runInjectors.js"],
  "icons": {
    "64": "icons/icon.png"
  }
}
